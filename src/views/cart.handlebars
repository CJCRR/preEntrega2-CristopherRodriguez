<h1>Detalles del carrito</h1>
    <table>
        <thead>
            <tr>
                <th>Producto</th>
                <th>Precio</th>
                <th>Cantidad</th>
            </tr>
        </thead>
        <tbody>

    {{#each cart.products}}
    <tr>
        <td>{{this._id}} - {{this.title}}</td>
        <td>{{this.price}} -</td>
        <td>{{this.quantity}}</td>
    </tr>
    {{/each}}

</tbody>
</tbody>



    </table>
